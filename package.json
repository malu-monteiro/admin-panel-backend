    "@types/nodemailer": "^6.4.17",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3",
    "ts-node": "^10.9.2"
  }
}
