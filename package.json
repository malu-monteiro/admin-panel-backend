{
  "name": "admin-panel",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/app.ts",
    "docker:up": "docker-compose up",
    "docker:build": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "build": "tsc",
    "build:seed": "tsc prisma/seed.ts --outDir dist/prisma --esModuleInterop --module commonjs --skipLibCheck",
    "start": "npx prisma migrate deploy && npx prisma db seed && node dist/app.js",
    "postinstall": "npx prisma generate"
  },
  "prisma": {
    "seed": "node dist/prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.2.2",
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "fastify": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "mailhog": "^4.16.0",
    "nodemailer": "^6.10.1",
    "pg": "^8.16.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
